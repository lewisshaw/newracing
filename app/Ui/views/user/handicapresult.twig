{% extends "user/layout.twig" %}
{% block breadcrumb %}
  {{ parent() }}
  <li>result</li>
{% endblock %}
{% block content %}

<h3>View handicap results</h3>

<table class="table table-striped table-hover">
  <thead>
    <tr>
    <th>Position</th><th>Sail Number</th><th>Helm</th><th>Crew</th><th>Boat Class Name</th><th>Time</th><th>Corrected Time</th>
    </tr>
  </thead>
  <tbody>
{% for result in results %}
<tr>
    <td>{{ result.position }}</td> <td>{{ result.sailNumber }}</td> <td>{{ result.helm }}</td> <td>{{ result.crew }}</td> <td>{{ result.boatClassName }}</td> <td>{{ result.time is defined ? result.time : result.unfinishedTypeHandle }}</td> <td>{{ result.correctedTime is defined ? result.correctedTime : result.unfinishedTypeHandle }}</td>
</tr>
{% endfor %}
</tbody>
</table>
{% endblock %}
