<table class="table table-striped table-hover">
  <thead>
    <tr>
    <th>Position</th><th>Sail Number</th><th>Helm</th><th>Crew</th><th>Boat Class Name</th>
    </tr>
  </thead>
  <tbody>
{% for result in results %}
<tr>
    <td>{{ result.position }} {{ result.unfinishedTypeHandle is defined ? '(' ~ result.unfinishedTypeHandle ~ ')' : '' }} </td> <td>{{ result.sailNumber }}</td> <td>{{ result.helm.fullName }}</td>
    <td>
      {% if result.crew|length > 0 %}
        {{ result.crew.fullName }}
      {%else %}
        No Crew
      {% endif %}
    </td>
    <td>{{ result.boatClassName }}</td>
</tr>
{% endfor %}
</tbody>
</table>
